<div class="container">
  <h1 id="title"><%= @title %></h1>

  <% @tickets.each do |ticket| %>
    <%= link_to ticket_path(ticket) do %>
    <div class="ticket-card">
      <div class="ticket-infos">
        <%= image_tag "data:image/png;base64,", class: "brand-img" %>
        <div class="ticket-shop">
          <p><strong>U Express</strong></p>
          <p><small><%= ticket.created_at.strftime("%d/%m/%Y") %></small></p>
          <% if ticket.items.where.not(product_id: nil).count > 1%>
            <p> <%= ticket.items.where.not(product_id: nil).count %> produits </p>
        <% else %>
            <p> <%= ticket.items.where.not(product_id: nil).count %> produits </p>
        <% end %>
        </div>
        <i class="fas fa-long-arrow-alt-right"></i>
      </div>
    </div>
    <% end %>
  <% end %>
</div>
