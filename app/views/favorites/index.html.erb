<h1>Vos favoris</h1>
<ul>
  <% @favorites.each do |favorite| %>
    <li>
      <%= favorite.product.name %>
        <%= link_to favorite_path(favorite, redirect_to: "index"), method: :delete do %>
          <i class="far fa-trash-alt" id="delete"></i>
          <% end %>
      </li>
  <% end %>
</ul>
